<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baibhab.sleep1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".date" />
        <activity
            android:name=".screen1"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".screen2" />
        <activity android:name=".screen3" />
        <activity android:name=".home" />
        <activity android:name=".showData" />
        <activity android:name=".CreatePasswordActivity" />
        <activity android:name=".EnterPasswordActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".showScore"></activity>
    </application>

</manifest>